<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--接收CSA01报文的mq通道-->
    <add key="ReceiveCSA01MqAddress" value=".\private$\receiveCSA01"/>
    <!--发送CSA01报文的mq通道-->
    <add key="SendCSA01MqAddress" value=".\private$\sendCSA01"/>
    <!--接收CSA02报文的mq通道-->
    <add key="ReceiveCSA02MqAddress" value=".\private$\receiveCSA02"/>
    <!--发送CSA02报文的mq通道-->
    <add key="SendCSA02MqAddress" value=".\private$\sendCSA02"/>

    <!--CSA01文件目录-->
    <add key="CSA01FilePath" value="F:\csafile\CSA01.xml"/>
    <!--CSA02文件目录-->
    <add key="CSA02FilePath" value="F:\csafile\CSA02.xml"/>
    
    <!--CSA01文件的保存目录-->
    <add key="CSA01FileSavePath" value="F:\csafile\CSA01"/>
    <!--CSA02文件的保存目录-->
    <add key="CSA02FileSavePath" value="F:\csafile\CSA02"/>
  </appSettings>
  <connectionStrings>
    <!--数据库连接-->
    <add name="SQLConnString" connectionString="server=.;database=CSATRANS;uid=sa;pwd=xmg20162401" />
  </connectionStrings>

  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <log4net debug="false">
    <appender name="ADONetAppender" type="log4net.Appender.ADONetAppender">
      <bufferSize value="1"/>
      <connectionType value="System.Data.SqlClient.SqlConnection, System.Data, Version=1.0.3300.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
      <connectionString value="server=.;database=CSATRANS;uid=sa;pwd=xmg20162401"/>
      <commandText value="INSERT INTO CSAOPERATIONINFO(GUID,RELGUID,MESSAGEID,MESSAGETYPE,OPERATETYPE,OPETATEDATE,OPERATERESULT,OPERATEREMAKR,LOGLEVEL) VALUES(@GUID,@RELGUID,@MESSAGEID,@MESSAGETYPE,@OPERATETYPE,@OPETATEDATE,@OPERATERESULT,@OPERATEREMAKR,@LOGLEVEL)"/>
      <parameter>
        <parameterName value="@GUID"/>
        <dbType value="uniqueidentifier"/>
        <layout type="log4net.Layout.RawTimeStampLayout"/>
      </parameter>
      <parameter>
        <parameterName value="RELGUID"/>
        <dbType value="uniqueidentifier"/>
        <layout type="log4net.Layout.PatternLayout"/>
      </parameter>
      <parameter>
        <parameterName value="MESSAGEID"/>
        <dbType value="String"/>
        <size value="64"/>
        <layout type="log4net.Layout.PatternLayout"/>
      </parameter>
      <parameter>
        <parameterName value="@MESSAGETYPE"/>
        <dbType value="String"/>
        <size value="10"/>
        <layout type="log4net.Layout.PatternLayout"/>
      </parameter>
      <parameter>
        <parameterName value="@OPERATETYPE"/>
        <dbType value="String"/>
        <size value="2"/>
        <layout type="log4net.Layout.ExceptionLayout"/>
      </parameter>
      <parameter>
        <parameterName value="@OPETATEDATE"/>
        <dbType value="DateTime"/>
        <layout type="log4net.Layout.RawTimeStampLayout"/>
      </parameter>
      <parameter>
        <parameterName value="@OPERATERESULT"/>
        <dbType value="String"/>
        <size value="2"/>
        <layout type="log4net.Layout.ExceptionLayout"/>
      </parameter>
      <parameter>
        <parameterName value="@OPERATEREMAKR"/>
        <dbType value="String"/>
        <size value="255"/>
        <layout type="log4net.Layout.ExceptionLayout"/>
      </parameter>
      <parameter>
        <parameterName value="@LOGLEVEL"/>
        <dbType value="String"/>
        <size value="50"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%level"/>
        </layout>
      </parameter>
    </appender>
    <root>
      <level value="DEBUG"/>
      <appender-ref ref="ADONetAppender"/>
    </root>
  </log4net>
</configuration>